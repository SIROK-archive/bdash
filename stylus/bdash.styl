/**
 * Please consider the specification of CSS. These stylus files are imported
 * on the basis of ITCSS so specifiency order is increasing.
 * Please see each section description.
 *
 * 1. settings ... Global variables, config switches
 * 2. tools ...... Default mixins, functions and tools
 * 3. generic .... Ground-zero style ( sanitize.css )
 * 4. base ....... Unclassed HTML element
 * 5. elements ... Objects and Components
 * 6. trumps ..... Overrides
 */


/**
 * Settings
 */
@import "settings/_vars"


/**
 * Tools
 */
@import "tools/_clearfix"
@import "tools/_nav"


/**
 * Generic
 */
@import "generic/_sanitize"


/**
 * Base
 */
@import "base/_base"


/**
 * Objects
 */
@import "objects/_ui-button"
@import "objects/_ui-switch"
@import "objects/_ui-pagination"
@import "objects/_ui-label"
@import "objects/_ui-table"
@import "objects/_ui-loading"
@import "objects/_ui-tabnav"




/**
 * Components
 */
@import "components/_buttons"
@import "components/_switch"
@import "components/_table"


/**
 * Layouts
 */
@import "layouts/_dashboard"


/**
 * Trumps
 */
@import "trumps/_media"
